{
  "name": "gitfolio",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "eslint": "^4.18.2",
    "eslint-config-expo": "^6.0.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-react": "^7.7.0",
    "jest-expo": "^25.1.0",
    "prettier": "1.11.1",
    "react-native-scripts": "1.11.1",
    "react-test-renderer": "^16.2.0"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "node node_modules/jest/bin/jest.js",
    "test:updateSnapshot": "node node_modules/jest/bin/jest.js --updateSnapshot",
    "lint": "eslint *.js tests/*.js components/*.js components/tests/*.js",
    "lint:fix": "eslint *.js tests/*.js components/*.js components/tests/*.js --fix"
  },
  "jest": {
    "preset": "jest-expo",
    "testRegex": "/.*.test(\\.js|\\.coffee|[^d]\\.ts)$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node",
      "coffee",
      "ts"
    ],
    "modulePaths": [
      ".",
      "./components",
      "./node_modules"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-navigation|native-base|@expo|react-native|expo|native-base-shoutem-theme|victory|react-native-svg)"
    ],
    "timers": "fake"
  },
  "dependencies": {
    "@expo/vector-icons": "^6.3.1",
    "apollo-client-preset": "^1.0.8",
    "apollo-link-context": "^1.0.7",
    "axios": "^0.18.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "expo": "^26.0.0",
    "github-colors": "^2.2.17",
    "graphql": "^0.13.1",
    "graphql-tag": "^2.8.0",
    "native-base": "^2.3.9",
    "node-emoji": "^1.8.1",
    "react": "^16.3.0",
    "react-native": "0.54.0",
    "react-native-hyperlink": "0.0.12",
    "react-native-modal": "^5.4.0",
    "react-navigation": "^1.4.0",
    "victory-native": "^0.17.3"
  }
}
